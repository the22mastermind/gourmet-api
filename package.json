{
  "name": "gourmet-api",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "https://github.com/the22mastermind/gourmet-api.git",
  "author": "the22mastermind <bertrandmasabo22@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "make-runnable": "^1.3.8",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "redis": "^3.0.2",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-test-helpers": "^1.3.2",
    "stripe": "^8.138.0",
    "twilio": "^3.54.1"
  },
  "devDependencies": {
    "@babel/register": "^7.12.10",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.6.0",
    "mocha": "^8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0"
  },
  "scripts": {
    "start": "babel-node ./src/server.js",
    "dev": "cross-env NODE_ENV=development && nodemon --exec babel-node ./src/server.js",
    "dev-test": "cross-env NODE_ENV=test yarn pretest && nyc mocha --require @babel/register ./tests/*.js --timeout 15000 --exit",
    "test": "cross-env NODE_ENV=test && nyc --reporter=lcov mocha --require @babel/register ./tests/*.js --timeout 15000 --exit",
    "create-db": "npx sequelize-cli db:create",
    "drop-db": "npx sequelize-cli db:drop",
    "migrate": "npx sequelize-cli db:migrate",
    "pretest": "yarn lint && yarn drop-db && yarn create-db && yarn migrate && yarn create-admin && yarn seed",
    "heroku-postbuild": "yarn migrate && yarn create-admin && yarn create-menu",
    "create-admin": "babel-node ./src/database/scripts/adminScript.js",
    "seed": "npx sequelize-cli db:seed:all",
    "create-menu": "babel-node ./src/database/scripts/menuScript.js",
    "lint": "yarn run eslint src/ && yarn run eslint tests/"
  },
  "engines": {
    "node": "14.x"
  }
}
